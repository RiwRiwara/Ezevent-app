{
  "expo": {
    "name": "ezevent-app",
    "scheme": "ezevent",
    "slug": "ezevent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.awirut352629.ezevent"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.awirut352629.ezevent"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/ibm-plex-sans-thai/IBMPlexSansThai_100Thin.ttf",
            "node_modules/@expo-google-fonts/ibm-plex-sans-thai/IBMPlexSansThai_200ExtraLight.ttf",
            "node_modules/@expo-google-fonts/ibm-plex-sans-thai/IBMPlexSansThai_300Light.ttf",
            "node_modules/@expo-google-fonts/ibm-plex-sans-thai/IBMPlexSansThai_400Regular.ttf",
            "node_modules/@expo-google-fonts/ibm-plex-sans-thai/IBMPlexSansThai_500Medium.ttf",
            "node_modules/@expo-google-fonts/ibm-plex-sans-thai/IBMPlexSansThai_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/ibm-plex-sans-thai/IBMPlexSansThai_700Bold.ttf"
          ]
        }
      ],
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cadbeaff-6298-4b77-96db-c576d01fc368"
      }
    },
    "owner": "awirut352629"
  }
}
